        "class Human:\n",
        "  def __init__(self, name:str, age:int):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前:{self.name}\")\n",
        "    print(f\"年齢:{self.age}\")\n",
        "\n",
        "person = Human(\"山田太郎\", 25)\n",
        "\n",
        "person.printinfo()"
